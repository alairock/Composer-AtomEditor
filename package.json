{
  "author": "Skylar Lewis",
  "contributors": [
    {
      "name": "Simon Fridlund"
    }
  ],
  "name": "composer",
  "main": "lib/main",
  "version": "0.4.2",
  "description": "Run Composer within Atom",
  "repository": "https://github.com/alairock/Composer-AtomEditor",
  "license": "MIT",
  "engines": {
    "atom": ">=0.196.0 <2.0.0"
  },
  "dependencies": {
    "atom-message-panel": "^1.2.0",
    "byline": "^4.2.1",
    "cross-spawn": "^0.2.9",
    "stringstream": "0.0.4"
  },
  "activationCommands": {
    "atom-workspace": [
      "composer:about",
      "composer:archive",
      "composer:clear-cache",
      "composer:dump-autoload",
      "composer:install",
      "composer:self-update",
      "composer:update",
      "composer:validate"
    ]
  }
}
